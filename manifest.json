{
  "name": "Spatial Navigation",
  "short_name": "spatialnavigation",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Spatial navigation.",
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "dist/raw/spatialnavigation.js"
  ],
  "permissions": [
    "<all_urls>",
    "activeTab"
  ]
}
