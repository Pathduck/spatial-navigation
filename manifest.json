{
  "name": "Spatial Navigation",
  "short_name": "spatialnavigation",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Spatial navigation. Bind WASDE and Q keys.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/dist/raw/content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "dist/raw/spatialnavigation.js"
  ],
  "permissions": [
    "<all_urls>",
    "activeTab"
  ]
}
